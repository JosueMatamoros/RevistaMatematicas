<script language="JavaScript">
function opw(text,menu,opc){                           
   nw= window.open("",opc);
   nw.document.write("<html><head></head>"
   +"<frameset rows='84,*' framespacing='0' border='0' frameborder='0'>"
   +"<frame name='banner' scrolling='no' noresize target='contents' src='SubProbaHeaderNew.htm'>"
   +"<frameset cols='148,*'>"
   +"<frame name='contents' target='main' src='"+menu+"'>"
   +"<frame name='main'  src='"+text+"' scrolling='auto' target='_self'>"
   +"</frameset>"
   +"<noframes><body></body></noframes></frameset></html>"
   );                            
}
</script>


</head>

